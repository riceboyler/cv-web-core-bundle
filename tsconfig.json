{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "baseUrl": "src",
    "moduleResolution": "node",
    "lib": ["dom", "es2016", "es2017.object"]
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
